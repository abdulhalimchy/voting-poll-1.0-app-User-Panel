<% if(typeof user != 'undefined') { %>
<%- include('./partials/header/after_login'); %>
<% } else { %>
<%- include('./partials/header/before_login'); %>
<% } %>

<div class="container mt-5">
    <div class="card shadow p-5 border-0">
        <h4 class="font-weight-bold mb-3 mt-1">Frequently Asked Questions</h4>
        <% if(typeof resultFaq != 'undefined'){ %>
            <% for(let i=0; i<resultFaq.length; i++){ %>
                <h6 class="font-weight-bold"><%= resultFaq[i].question %></h6>
                <p class="text-muted">
                    <%= resultFaq[i].answer %>
                </p>
            <% } %>
        <% } %>
    </div>
</div>

